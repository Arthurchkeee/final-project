<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
        version="4.0">

    <!-- Config here. -->
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>catalog</servlet-name>
        <servlet-class>com.epam.project.servlets.CatalogServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>catalog</servlet-name>
        <url-pattern>/catalog</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>loginUser</servlet-name>
        <servlet-class>com.epam.project.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginUser</servlet-name>
        <url-pattern>/LoginUser</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>logoutUser</servlet-name>
        <servlet-class>com.epam.project.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logoutUser</servlet-name>
        <url-pattern>/LogoutUser</url-pattern>
    </servlet-mapping>
    <jsp-config>
        <taglib>
            <taglib-uri>uri</taglib-uri>
            <taglib-location>WEB-INF/tag.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>deleteUserButton</taglib-uri>
            <taglib-location>WEB-INF/deleteUserButton.tld</taglib-location>
        </taglib>
    </jsp-config>


</web-app>